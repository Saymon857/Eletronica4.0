#include <stdio.h>

int main(void)
{
  char letra;
  
  printf("Escolha uma letra: ");
  scanf("%c%*c",&letra);
  switch(letra){
     case 'a':
        printf(" Argentina");
        break;
    return 0;
  }
  }
