#include <stdio.h>
#include <stdlib.h> //needed for the functions rand() and srand()
#include <time.h> //needed for the function time()

int main(void)
{
int i;
srand(time(NULL));
for (i = 1; i<11; i++)
printf("%4d", rand() % 1000 + 1);
 printf(" \n");
return 0;
 }
