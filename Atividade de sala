#include <stdio.h>

int main(void)
{
    int número;
    printf("Insira um número: ");
    scanf("%d", &número);
    if (número % 2 == 0)
    {
        printf("%d é par", número);
        
    }
    else
    {
        printf("%d é impar",número);
        
    }
    return 0;
}
